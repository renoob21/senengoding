---
import hljs from "highlight.js";
import "highlight.js/styles/tokyo-night-dark.css"

const {code, lang} = Astro.props;

const coded = hljs.highlight(code, {language:lang}).value;
console.log(coded);
---


<pre class="hljs font-mono astro-code">
    <code>
        <Fragment set:html={coded}/>
    </code>
</pre>
